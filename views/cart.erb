<h2>Cart</h2>
<table class="table table-striped">
  <thead>
  	<tr>
      <th scope="col">Title</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
  	<% @products.each do |product| %>
	<tr>
		<td><%= product.title %></td>
		<td><%= product.price %></td>
	</tr>
	<% end %>
</tbody>
</table>

